<h1>New Booking</h1>

<%= form_for @booking do |f| %>

    <div class="form-group">
        <%= f.label :from_address %>
        <%= f.text_field :from_address, :class => 'form-control'  %>
    </div>

    <div class="form-group">
        <%= f.label :to_address %>
        <%= f.text_field :to_address, :class => 'form-control' %>
    </div>

    <div class="form-group">
        <%= f.label :estimated_price %>
        <%= f.text_field :estimated_price, :class => 'form-control' %>
    </div>

    <%= f.submit "Create booking", :class => 'btn btn-success btn-lg' %>

<% end %>